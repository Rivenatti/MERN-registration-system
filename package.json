  "name": "registration-system",
  "version": "1.0.0",
  "engines": {
    "node": "8.11.3",
    "npm": "5.6.0"
  },
  "description": "## Get started",
  "main": "server.js",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "jwt-decode": "^2.2.0",
    "mongoose": "^5.3.14",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "nodemon": "^1.18.7"
  },
  "scripts": {
    "start": "concurrently \"node server.js\" \"npm start --prefix client\"",
    "client": "npm start --prefix client",
    "client-install": "npm install --prefix client",
    "dev": "concurrently \"set NODE_ENV=development&& nodemon run server \" \"npm run client \"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rivenatti/mern-registration-system.git"
  }
}
